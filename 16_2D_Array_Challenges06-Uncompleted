EmptyArray = [[0 for colummn in range(5)] for now in range(2)]
print(EmptyArray)

num1 = int(input("Choose a number to be the range"))
num2 = int(input("Choose another number to be the other range"))
EmptyArray = [["a" for column in range(num1)] for now in range(num2)]
print(EmptyArray)

Array = int(input("Choose a number to be the range"))
Character = int(input("Choose another number to be the other range"))
EmptyArray[Array-1][Character-1] = str(input("Enter a new value"))
print(EmptyArray)
