answer = input("Is it raining?")
if answer == "yes".lower():
    answer1 = input("Is it windy?")
    if answer1 == "yes".lower():
        print("It is too windy for an umbrella.")
    elif answer1 == "no".lower():
        print("take an umbrella")
elif answer == "no".lower():
    print("Enjoy your day")
